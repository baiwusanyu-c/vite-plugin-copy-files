{
  "name": "vite-plugin-copy-files",
  "version": "1.1.1",
  "packageManager": "pnpm@7.7.0",
  "description": "This package can help you move some files that do not need to be packaged to the corresponding packaging directory",
  "keywords": [
    "vite-plugin",
    "vite"
  ],
  "license": "MIT",
  "author": "aibayanyu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistjs/vite-plugin-copy-files.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "vitest",
    "test:u": "vitest -u",
    "test:run": "vitest run",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "playground": "vite playground",
    "playground:build": "vite build playground",
    "changelog": "npx changelogen"
  },
  "bugs": {
    "url": "https://github.com/mistjs/vite-plugin-copy-files/issues"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1"
  },
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@mistjs/eslint-config": "^0.0.2",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "changelogen": "^0.0.3",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "magic-string": "^0.26.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^3.2.2",
    "vitest": "^0.9.4",
    "vue": "^3.2.33"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": "eslint --fix --ext .ts,.tsx,.js,.vue"
  },
  "homepage": "https://github.com/mistjs/vite-plugin-copy-files#readme"
}
